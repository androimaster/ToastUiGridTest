<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="common">
    
  <cache />
  
  <select id="selectMembers" resultType="hashmap">
	SELECT NAME
         , EMAIL
         , GENDER
         , HOBBY1
         , HOBBY2
      FROM MEMBERS
  </select>
</mapper>
